{
	"manifest_version": 2,
	"name": "QuickerLinks",
	"short_name": "QuickerLinks",
	"version": "0.0.0.1",
	"description": "An experimental chrome extension built by D2L Labs. This extension is not supported and is provided as-is, and without warranty.",
	"omnibox": { "keyword" : "d2l" },
	"icons": {
		"16": "./styles/images/icon.png"
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
   		"activeTab",
   		"tabs",
   		"http://*/*",
   		"https://*/*",
		"history",
		"tabs"
   	],
	"background": {
    	"scripts": ["scripts/quickerlinks.js", "scripts/course.js", "scripts/omnibox.js"],
     	"persistent": false
   	}
}
